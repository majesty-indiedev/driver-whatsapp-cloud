{
    "name": "mohapinkepane/driver-whatsapp-cloud",
    "license": "MIT",
    "description": "WABA Cloud API driver for BotMan",
    "keywords": [
        "Bot",
        "BotMan",
        "WhatsApp",
        "WABA"
    ],
    "homepage": "https://github.com/mohapinkepane/driver-whatsapp-cloud",
    "authors": [
        {
            "name": "Mohapi Nkepane",
            "email": "mohapi765@gmail.com",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "botman/botman": "^2.0",
        "ext-curl": "*",
        "phpseclib/phpseclib": "^3.0"
    },
    "require-dev": {
        "botman/studio-addons": "^1.0",
        "illuminate/contracts": "^5.5",
        "phpunit/phpunit": "^5.0",
        "mockery/mockery": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "BotMan\\Drivers\\Whatsapp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "cs": "php-cs-fixer fix"
    },
    "extra": {
        "laravel": {
            "providers": [
                "BotMan\\Drivers\\Whatsapp\\Providers\\WhatsappServiceProvider"
            ]
        }
    }
  }